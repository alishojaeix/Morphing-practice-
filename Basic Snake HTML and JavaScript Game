    <span class="pl-kos">}</span>

    <span class="pl-c1">randomAppear</span> <span class="pl-c1">=</span> <span class="pl-k">function</span><span class="pl-kos">(</span><span class="pl-s1">isCollision</span><span class="pl-kos">)</span><span class="pl-kos">{</span>
